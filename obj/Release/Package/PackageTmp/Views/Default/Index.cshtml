<!DOCTYPE HTML>
<html>

<body>
    
    
    <div class="header-banner">
        <div class="wrap">
            <div class="banner">
                <div class="indent">
                    <h2 class="indent-left margin-bot">Plan the Easy Trip</h2>
                    <div class="banner-left">
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/glass.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Restaranlar</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/flight.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Ucuslar</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/group.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Gorulecek yerler</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="con-text">
                            <div class="grid1-img">
                                <img src="~/web/images/key.png" alt="">
                            </div>
                            <div class="grid1-desc">
                                <p>Icareye masinlar</p>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <div class="clear"></div>
                    </div>

                    <div class="header_bottom_right_images">

                        <div class="slider">

                            <ul class="rslides" id="slider1">
                                @foreach (var w in Model.deger1)
                                {
                                    <li><img src="@w.BLOGIMAGE" style="width:550px;height:400px" alt=""></li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>

                <div class="clear"></div>
            </div>
          
            <div class="clear"></div>
        </div>
    </div>
    <div class"content">
        <div class="wrap">
            <div class="wrapper">
                <div class="con-top">
                    <h3>Seyahet blogma xos geldiniz</h3>
                    
                    <p>Gezib gorduymuz yerleri sizlerle paylasmaq ucun kodlamasini tamami ile ozumuz etdik
                </div>
                @Html.Action("partial","Default")
               
                <div class="grids">
                    <div class="box1">

                        @Html.Action("partial3", "Default")


                        @Html.Action("partial4", "Default")

                        <div class="clear"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</body>
</html>
